<Window x:Class="GenderHealthCareSystem.Dashboard.ManagerDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenderHealthCareSystem.Dashboard"
        xmlns:stis="clr-namespace:GenderHealthCareSystem.ServiceMangement" 
        mc:Ignorable="d"
        Title="ManagerDashboard" Height="450" Width="800">
    <Grid>
        <TabControl Margin="10">
            <TabItem Header="Quản lý Dịch vụ Stis">
                <stis:StisServiceWindow/>
            </TabItem>
        </TabControl>
    </Grid>
    <StackPanel>
        <Button x:Name="btnManageStisBooking" Content="Manage STIs Booking" Click="btnManageStisBooking_Click" Margin="10"/>
    </StackPanel>
</Window>
